
{
    "name": "coderflex/laravisit",
    "description": "This is my package laravisit",
    "keywords": [
        "coderflex",
        "laravel",
        "laravisit"
    ],
    "homepage": "https://github.com/coderflexx/laravisit",
    "license": "MIT",
    "authors": [
        {
            "name": "coderflex",
            "email": "oussama@coderflex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-de/v": {
        "nunomaduro/collision": "^5.10|^6.1",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.23|^7.0.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Coderflex\\Laravisit\\": "src",
            "Coderflex\\Laravisit\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Coderflex\\Laravisit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coderflex\\Laravisit\\LaravisitServiceProvider"
            ],
            "aliases": {
                "Laravisit": "Coderflex\\Laravisit\\Facades\\Laravisit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
